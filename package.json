{
  "name": "gpt3-vscode-extension",
  "displayName": "GPT A.I. Coding Assistant",
  "description": "Use GPT-3 to improve your code and generate documentation!",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning"
  ],
  "keywords": [
    "AI",
    "OpenAI",
    "GPT-3",
    "GPT3",
    "GPT"
  ],
  "activationEvents": [
    "onCommand:GPT.createDocs",
    "onCommand:GPT.suggestImprovement"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GPT.createDocs",
        "title": "Create Documentation"
      },
      {
        "command": "GPT.suggestImprovement",
        "title": "Suggest Improvements"
      }
    ],
    "keybindings": [
      {
        "command": "GPT.createDocs",
        "key": "alt+d",
        "mac": "option+d",
        "when": "editorTextFocus"
      },
      {
        "command": "GPT.suggestImprovement",
        "key": "alt+i",
        "mac": "option+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arrendy/gpt3-vscode-extension.git"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "author": "Mike Bird",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Arrendy/gpt3-vscode-extension/issues"
  },
  "homepage": "https://github.com/Arrendy/gpt3-vscode-extension#readme",
  "dependencies": {
    "openai": "^3.1.0"
  },
  "pricing": "Free",
  "publisher": "Arrendy",
  "icon": "images/icon.png"
}
